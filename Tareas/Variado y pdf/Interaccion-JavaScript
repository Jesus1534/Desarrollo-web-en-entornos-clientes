    <input class="boton" type="button" id="boton1" value="boton" onclick="boton1()"/>
    <input class="boton" type="button" id="boton2" value="boton" onclick="boton2()"/>

    <script>
        function boton1() {
            alert("Has pulsado el botón 1");
            console.log("Consola del botón 1");
        }
        function boton2() {
            alert("Pulsaste el boton 2");
            console.log("Consola del botón 2");
        }
    </script>

